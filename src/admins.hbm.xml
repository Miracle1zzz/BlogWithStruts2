<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>


<class name="com.hulin.model.Admin" table="admins">

<id name="id" column="id" type="int">
		<generator class="increment"></generator>
	</id>
	
	<property name="admins_name" column="admins_name" type="string"></property>
	<property name="admins_pwd" column="admins_pwd" type="string"></property>
	<property name="add_date" column="add_date" type="string"></property>
	<property name="admins_level" column="admins_level" type="string"></property>
	
	
</class>

</hibernate-mapping>