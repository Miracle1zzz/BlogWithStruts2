<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>

<class name="com.hulin.model.User" table="users">

<id name="id" column="id" type="int">
		<generator class="increment"></generator>
	</id>
	
	<property name="user_name" column="user_name" type="string"></property>
	<property name="user_pwd" column="user_pwd" type="string"></property>
	<property name="user_sex" column="user_sex" type="string"></property>
	<property name="user_qq" column="user_qq" type="string"></property>
	<property name="user_email" column="user_email" type="string"></property>


</class>
</hibernate-mapping>
